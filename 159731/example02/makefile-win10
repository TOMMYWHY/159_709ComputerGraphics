HEADERS = -I./GLFW/include/ -I./GLEW/include/
LIBS = -L./GLFW/lib/ -L./GLEW/lib/ -lglew32 -lglfw3 -lopengl32 -lgdi32
CC = g++ -O3 -Wall -W

example02: example02.o
	${CC} example02.o -o example02 ${LIBS}

example02.o: example02.cpp
	${CC} -c example02.cpp -o example02.o ${HEADERS}

clean:
	del *.o
	del example02